{
    "name": "ypho/scryfall",
	"homepage": "http://www.ypho.nl",
	"keywords": ["scryfall", "scryfall.com", "api wrapper"],
    "description": "A wrapper around the API from Scryfall (scryfall.com)",
    "require": {
	    "php": ">=7.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "~6.3",
        "symfony/dom-crawler": "*",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Ypho\\Scryfall\\": "src",
            "Ypho\\Scryfall\\Endpoint\\": "src/Endpoint",
            "Ypho\\Scryfall\\Exception\\": "src/Exception",
            "Ypho\\Scryfall\\Responses\\": "src/Responses"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ivo Geersen",
            "email": "ivo@ypho.nl"
        }
    ]
}
